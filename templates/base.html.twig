<!DOCTYPE html>
<html lang="fr">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>
        {% block title %}Accueil
        {% endblock %}
    </title>
    <link rel="icon" href="{{ asset('assets/dsfr/dist/favicon/favicon.svg') }}" type="image/svg+xml">
    <link rel="manifest" href="{{ asset('assets/dsfr/dist/favicon/manifest.webmanifest') }}"
          crossorigin="use-credentials">
    <link rel="shortcut icon" href="{{ asset('assets/dsfr/dist/favicon/favicon.ico') }}" type="image/x-icon">
    {% block stylesheets %}
        <link rel="stylesheet" href="https://bootswatch.com/5/lumen/bootstrap.min.css">
        <link rel="stylesheet" href="{{ asset('assets/dsfr/dist/dsfr.min.css') }}">
    {% endblock %}
</head>
<body>
{% include "shared/_navbar.html.twig" %}
<div class="container pt-5"> {% block body %}{% endblock %}
</div>
{% block javascripts %}
    <script type="module" src="{{ asset('assets/dsfr/dist/dsfr.module.min.js') }}"></script>
    <script type="text/javascript" nomodule src="{{ asset('assets/dsfr/dist/dsfr.nomodule.min.js') }}"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/bootstrap/5.1.3/js/bootstrap.bundle.min.js"></script>
{% endblock %}
</body>
</html>
